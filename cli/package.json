{
  "name": "claude-crew",
  "version": "1.0.0",
  "description": "CLI tool for managing Claude Code agents - generate social images and list agents",
  "author": "Astralform AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom2ueki/claude-crew.git"
  },
  "homepage": "https://github.com/atom2ueki/claude-crew#readme",
  "bugs": {
    "url": "https://github.com/atom2ueki/claude-crew/issues"
  },
  "keywords": [
    "claude",
    "claude-code",
    "cli",
    "agents",
    "ai",
    "subagents"
  ],
  "type": "module",
  "bin": {
    "claude-crew": "./dist/index.cjs"
